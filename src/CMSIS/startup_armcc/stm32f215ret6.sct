; *************************************************************
; *** Scatter-Loading Description File (STM32F215RET6)        ***
; *************************************************************

LR_IROM1 0x08000000 0x00080000  {    ; 512 KB Flash
  ER_IROM1 0x08000000 0x00080000  {  ; load address = execution address
    *.o (RESET, +First)
    *(InRoot$$Sections)
    .ANY (+RO)
    .ANY (+XO)
  }

  RW_IRAM1 0x20000000 0x00020000  {  ; 128 KB SRAM
    .ANY (+RW +ZI)
  }
}
